#include <stdio.h>

int main()
{
    int temp,n,sum=0,rem;
    printf("Enter a number: ");
    scanf("%d",&n);
    temp=n;
    while(n>0)
    {
        rem=n%10;
        sum = sum+rem*rem*rem;
        n=n/10;
    }
    if(temp==sum)
    
        printf("%d is Armstrong number",temp);
        
    else
     printf("%d is not an armstrong number",temp);
     

    return 0;
}
