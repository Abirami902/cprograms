#include <stdio.h>

int main()
{
    int A,B,C;
    printf("Enter three numbers");
    scanf("%d%d%d",&A,&B,&C);
    if(A>B && A>C) 
    {
       printf("LARGEST NUMBER IS %d",A);
    }
    else if(B>C && B>A)
    {
      printf("LARGEST NUMBER IS  %d",B);  
    }

    else
   {
  printf("LARGEST NUMBER IS C  %d",C);
   }
    return 0;
}
