#include <stdio.h>
int main()

{
    int temp,n,sum=0,rem;
    printf("Enter a number: ");
    scanf("%d",&n);
    temp=n;
    
    while(n>0)
    {
        rem=n%10;
        sum = sum*10+rem;
        n=n/10;
    }
    if(temp==sum){
        printf("%d is palindrome  number",temp);
    }
    else{
    printf("Not palindrome");
    }

    return 0;
}
