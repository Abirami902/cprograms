#include <stdio.h>

int main()
{
    float p,r,t,SI;
    printf("Enter principal :");
    scanf("%f",&p);
     printf(" Enter Rate of Interest  :");
    scanf("%f",&r);
    printf("Enter Time in years:");
    scanf("%f",&t);
    SI = (p*r*t)/100;
    printf("Simple Interest :%f",SI);

    return 0;
}


