#include <stdio.h>

int main() {
    int array[6] = {4, 3, 4, 7, 4};
    int n = 5;
    int value = 4;

    printf("Original array: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", array[i]);
    }
    
    printf("\n");

    printf("Array with %d removed: ", value);
    for (int i = 0; i < n; i++) {
        if (array[i] != value) {
            printf("%d ", array[i]);
        }
    }
    printf("\n");

    return 0;
}
