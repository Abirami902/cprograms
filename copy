#include <stdio.h>

int main()
{
    int arr[25],c[25],i,n;
    
    printf("Enter number of elements  :");
    scanf("%d",&n);
    
    printf("Enter the elements [%d] :",n);
    
    for(i=0;i<n;i++){
    scanf("%d",&arr[i]);}
    
    	printf("The first array is :"); 
	for (i = 0; i < n; i++) { 
		printf("%d ", arr[i]); }
		
		
		for(i=0;i<n;i++){
		c[i]=arr[i];}
		
			printf("\nThe copied array is :"); 
	for (i = 0; i < n; i++) { 
		printf("%d ", c[i]); 
	} 


    return 0;

}
