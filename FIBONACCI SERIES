#include <stdio.h>

int main()
{
    int f1,f2,f3,i,n;
    printf("Enter the limit :");
    scanf("%d",&n);
    
    printf("FIBONACCI SERIES\n");
        f1=0;
        f2=1;
        
    for(i=3;i<=n;i++)
    {
        printf("%d\n",f3);
        f1=f2;
        f2=f3;
        f3=f1+f2;
        
    }
    return 0;
}
